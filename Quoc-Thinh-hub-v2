MainSection:NewButton("Speed Run 4 All Stage", "Completes All Stages" , function()
	while true do
 
		local args = {
			[1] = "Level 1",
			[2] = "Normal",
			[3] = false
		}
 
		game:GetService("ReplicatedStorage"):WaitForChild("BeatLevel"):FireServer(unpack(args))
 
		wait(0)
	end
end)
 
